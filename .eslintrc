{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "esnext": true
  },
  "rules": {
    "curly": [1,"all"],
    "strict": 0,
    "quotes": [1,"single"],
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0
  }
}
